let N := 10;

let i := 1;
let p := { 0, 1 };

let result := 0;

if N = 0 then
    result := p[0];
else
    if N = 1 then
        result := p[1];
    fi
fi

if N > 1 then
    let r := 0;
    while i < N do
        r := p[0] + p[1];
        p[0] := p[1];
        p[1] := r;
        i := i + 1;
    od

    result := p[1];
fi
