let N := 500;

let i := 1;
let p1 := 0;
let p2 := 1;

let result := 0;

if N = 0 then
    result := p1;
else
    if N = 1 then
        result := p2;
    fi
fi

if N > 1 then
    let r := 0;
    while i < N do
        r := p1 + p2;
        p1 := p2;
        p2 := r;
        i := i + 1;
    od

    result := p2;
fi
